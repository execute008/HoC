; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="hoc"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.5", "Mobile")
config/icon="res://icon.svg"

[autoload]

Utils="*res://scripts/utils.gd"
XRToolsUserSettings="*res://addons/godot-xr-tools/user_settings/user_settings.gd"
XRToolsRumbleManager="*res://addons/godot-xr-tools/rumble/rumble_manager.gd"
PerformanceManager="*res://scripts/performance_manager.gd"
PanelRegistry="*res://scripts/panel_registry.gd"
ProjectConfig="*res://scripts/project_config.gd"
BridgeLauncher="*res://scripts/bridge_launcher.gd"
BridgeClient="*res://scripts/bridge_client.gd"
AgentOrchestrator="*res://scripts/agent_orchestrator.gd"
InputRouter="*res://scripts/input_router.gd"
LayoutManager="*res://scripts/layout_manager.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-xr-tools/plugin.cfg")

[godot_openxr_vendors]

meta/application_mode=0
meta/hand_tracking=0
meta/hand_tracking_frequency=1
meta/passthrough=1
meta/enable_anchor_persistence=true
meta/enable_anchor_sharing=true
meta/request_runtime_permissions_automatically=true
meta/supported_devices=PackedStringArray("quest3", "quest3s", "questpro")

[layer_names]

3d_physics/layer_1="Static World"
3d_physics/layer_2="Dynamic World"
3d_physics/layer_3="Pickable Objects"
3d_physics/layer_4="Wall Walking"
3d_physics/layer_5="Grappling Target"
3d_physics/layer_17="Held Objects"
3d_physics/layer_18="Player Hands"
3d_physics/layer_19="Grab Handles"
3d_physics/layer_20="Player Body"
3d_physics/layer_21="Pointable Objects"
3d_physics/layer_22="Hand Pose Areas"
3d_physics/layer_23="UI Objects"

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
lights_and_shadows/directional_shadow/soft_shadow_filter_quality=0
lights_and_shadows/positional_shadow/soft_shadow_filter_quality=0
environment/defaults/default_clear_color=Color(0.1, 0.1, 0.12, 1)

[xr]

openxr/enabled=true
openxr/foveation_level=3
openxr/foveation_dynamic=true
openxr/submit_depth_buffer=true
openxr/startup_alert=false
openxr/extensions/hand_tracking=true
shaders/enabled=true
